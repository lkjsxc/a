struct obj_t (
    var id: i64
)

struct pi_t (
    var a: i64*
)

struct stat_t (
    struct pi_t (
        var a: i64
        var b: i64
    )
    struct gobj_t (
        var origin: pi_t
        var root: obj_t
    )
    var data: gobj_t
    fn debug(var stat: stat_t*) -> i64 (
        5 = stat->root.id
        return stat->root.id
    )
)

fn main() -> i64 (
    var stat: stat_t
    stat.debug()
)
